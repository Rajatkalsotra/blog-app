<% include ./partials/header %>

  <div id="contact" class="paddsection">
      <div class="container">
        <div class="contact-block1">
            <h2 class="mb-30">LOG IN</h2> 
              <form action="/login" method="POST" role="form" class="contactForm">
                <div class="row">
                  <div class="col-lg-12">
                    <div class="form-group contact-block1">
                      <input type="text" name="username" class="form-control" id="name" placeholder="Your Name*" required="required"/>   
                  </div>
                  </div>
                  <div class="col-lg-12">
                    <div class="form-group contact-block1">
                      <input type="password" name="password" class="form-control" placeholder="PASSWORD*" required="required"/>           
                    </div>
                  </div>
                  <div class="col-lg-2">
                    <input type="submit" class="btn btn-defeault btn-send" value="Submit">
                  </div>
                </div>
              </form>
            </div>
          </div>
        </div>
      </div>
    </div>

<% include ./partials/footer %>